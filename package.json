{
  "name": "physio-tracker-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "eslint src --ext .js,.jsx --max-warnings=0",
    "format": "prettier -w .",
    "audit:accessibility": "lighthouse http://localhost:4173 --only-categories=accessibility --quiet --chrome-flags=--headless --output=json --output-path=.lighthouse-accessibility.json",
    "analyze": "vite build --mode analyze"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,css,md}": [
      "prettier -w",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.548.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.13.2",
    "tailwindcss": "^3.4.13",
    "zustand": "^5.0.8"
  }
}
